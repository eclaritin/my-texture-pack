{
	"type": "minecraftBedrock",
	"name": "Eclair's Textures",
	"namespace": "eclair",
	"authors": [
		"Eclair"
	],
	"targetVersion": "1.21.0",
	"description": "A texture pack with some stuff I like",
	"experimentalGameplay": {
		"holidayCreatorFeatures": false,
		"creationOfCustomBiomes": false,
		"additionalModdingCapabilities": false,
		"upcomingCreatorFeatures": false,
		"enableGameTestFramework": false,
		"experimentalMolangFeatures": false,
		"theWildUpdate": false,
		"educationEdition": false
	},
	"packs": {
		"resourcePack": "./RP"
	},
	"worlds": [
		"./worlds/*"
	],
	"compiler": {
		"plugins": [
			"typeScript",
			"entityIdentifierAlias",
			"customEntityComponents",
			"customItemComponents",
			"customBlockComponents",
			"customCommands",
			"moLang",
			[
				"simpleRewrite",
				{
					"packName": "Eclair's Textures"
				}
			],
			"formatVersionCorrection"
		]
	},
	"bridge": {
		"formatVersion": 1
	}
}